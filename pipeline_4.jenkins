pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Test'){
            steps {
                sh 'pytest tests1.py'
            }
        }
        stage('Merge to relese'){
            steps {
                sh 'sh merge-to-release.sh'
            }
        }
        stage('Merge to main'){
            steps {
                sh 'sh merge-to-main.sh'
            }
        }
    }
}